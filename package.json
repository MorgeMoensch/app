{
  "name": "orbitingapp",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "run-android": "node ./scripts/run-android.js",
    "run-ios": "node ./scripts/run-ios.js",
    "run-android-dev": "ENVFILE=.env.dev react-native run-android",
    "run-android-staging": "ENVFILE=.env.staging react-native run-android",
    "run-android-production": "ENVFILE=.env.production react-native run-android",
    "run-ios-dev": "react-native run-ios --scheme 'orbitingapp (dev)'",
    "run-ios-staging": "react-native run-ios --scheme 'orbitingapp (staging)'",
    "run-ios-production": "react-native run-ios --scheme 'orbitingapp (production)'",
    "deploy-android-production": "cd ./android && ENVFILE=.env.production fastlane production",
    "deploy-android-staging": "cd ./android && ENVFILE=.env.staging fastlane staging",
    "deploy-ios-staging": "cd ./ios && fastlane ios staging",
    "deploy-ios-production": "cd ./ios && fastlane ios production",
    "release-ota": "node scripts/ota/release",
    "translations": "gsheets --key=1KFxXKjFplXM97N9ssk55oJol0FttDfwYiR9MJSQwpqw --title=live --pretty --out src/utils/translations.json",
    "standardize": "standard --fix",
    "bundle-into-simulator-app": "react-native bundle --platform ios --entry-file index.js --bundle-output ./ios/build/Build/Products/Debug-iphonesimulator/orbitingapp.app/main.jsbundle --assets-dest ./ios/build/Build/Products/Debug-iphonesimulator/orbitingapp.app/ --dev false",
    "cloc": "find . -name '*.js' -not -path \"./node_modules*\" | xargs wc -l"
  },
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "git add"
    ]
  },
  "rnpm": {
    "assets": [
      "./src/assets/fonts/"
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "dependencies": {
    "@project-r/styleguide": "^6.22.0",
    "apollo-cache-inmemory": "^1.2.1",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "^2.3.1",
    "apollo-link": "^1.2.4",
    "apollo-link-http": "^1.5.7",
    "apollo-link-state": "^0.4.2",
    "apollo-link-ws": "^1.0.10",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.10.1",
    "lodash.debounce": "^4.0.8",
    "react": "^16.7.0",
    "react-apollo": "^2.4.1",
    "react-json-tree": "^0.11.2",
    "react-native": "^0.58.3",
    "react-native-config": "^0.11.7",
    "react-native-cookies": "^3.3.0",
    "react-native-device-info": "^0.21.5",
    "react-native-firebase": "^4.2.0",
    "react-native-haptic": "^1.0.1",
    "react-native-notifications": "^1.2.51",
    "react-native-splash-screen": "3.2",
    "react-native-track-player": "^1.1.2",
    "react-native-wkwebview-reborn": "https://github.com/orbiting/react-native-wkwebview#update-2",
    "react-native-zip-archive": "^2.2.6",
    "react-navigation": "^2.18.3",
    "rn-fetch-blob": "^0.10.15",
    "subscriptions-transport-ws": "^0.9.15"
  },
  "devDependencies": {
    "archiver": "^2.1.1",
    "aws-sdk": "^2.374.0",
    "babel-eslint": "^8.2.6",
    "child-process-promise": "^2.2.1",
    "debug": "^3.2.6",
    "dotenv": "^6.2.0",
    "eslint": "^4.17.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^3.0.1",
    "gsheets": "^2.0.0",
    "husky": "^0.14.3",
    "inquirer": "^5.2.0",
    "isomorphic-unfetch": "^2.1.1",
    "lint-staged": "^6.1.0",
    "mkdirp": "^0.5.1",
    "prompt-checkbox": "^2.2.0",
    "prompt-confirm": "^2.0.4",
    "react-native-cli": "^2.0.1",
    "standard": "^12.0.1"
  }
}
